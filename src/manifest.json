{
  "manifest_version": 2,
  "name": "langhash",
  "version": "VERSION",
  "description": "pwdhash alternative",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "langhash",
    "default_popup": "extension.html"
  },
  "applications": {
    "gecko": {
      "id": "langhash@sjoerdlangkemper.nl"
    }
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+L"
      }
    }
  }
}
